{
  "name": "@worldql/client",
  "version": "0.1.1",
  "description": "TypeScript Client for interacting with WorldQL over WebSockets",
  "packageManager": "yarn@3.1.1",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts",
      "require": "./cjs/index.js"
    }
  },
  "main": "./cjs/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib/**/*",
    "cjs/**/*"
  ],
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "keywords": [
    "worldql",
    "typescript",
    "client",
    "websockets",
    "message broker",
    "spatial database"
  ],
  "scripts": {
    "prepack": "yarn run build",
    "prepublish": "yarn run test",
    "test": "yarn run check && yarn run lint",
    "check": "yarn run compile:esm --noEmit",
    "lint": "xo ./src",
    "clean:esm": "rimraf lib",
    "clean:cjs": "rimraf cjs",
    "clean": "yarn run clean:esm && yarn run clean:cjs",
    "compile:esm": "tsc",
    "compile:cjs": "tsc --outDir cjs --module commonjs",
    "build:esm": "yarn run clean:esm && yarn run compile:esm",
    "build:cjs": "yarn run clean:cjs && yarn run compile:cjs",
    "build": "yarn run build:esm && yarn run build:cjs",
    "dev": "yarn run clean:esm && yarn run compile:esm --watch"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.11",
    "@types/uuid": "^8.3.3",
    "@types/ws": "^8.2.1",
    "rimraf": "^3.0.2",
    "type-fest": "^2.8.0",
    "typescript": "^4.5.2",
    "xo": "^0.47.0"
  },
  "dependencies": {
    "@msgpack/msgpack": "^2.7.1",
    "eventemitter3": "^4.0.7",
    "isomorphic-ws": "^4.0.1",
    "ws": "^8.3.0"
  }
}
